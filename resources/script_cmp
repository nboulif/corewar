#-----------------------1 Player---------------------------------------------
#Gagnant
./a.out resources/tests/Gagnant.cor -r > rGagnant
./resources/vm_champs/corewar resources/tests/Gagnant.cor -v 4 > v4Gagnant
diff rGagnant v4Gagnant > diff_rv4Gagnant
echo "Gagnant"
cat diff_rv4Gagnant | grep "P "

#ultima
./a.out resources/tests/ultima.cor -r > rultima
./resources/vm_champs/corewar resources/tests/ultima.cor -v 4 > v4ultima
diff rultima v4ultima > diff_rv4ultima
echo "ultima"
cat diff_rv4ultima | grep "P "

#Kitty_Miaou
./a.out resources/tests/Kitty_Miaou.cor -r > rKitty_Miaou
./resources/vm_champs/corewar resources/tests/Kitty_Miaou.cor -v 4 > v4Kitty_Miaou
diff rKitty_Miaou v4Kitty_Miaou > diff_rv4Kitty_Miaou
echo "Kitty_Miaou"
cat diff_rv4Kitty_Miaou | grep "P "

#Backward
./a.out resources/tests/Backward.cor -r > rBackward
./resources/vm_champs/corewar resources/tests/Backward.cor -v 4 > v4Backward
diff rBackward v4Backward > diff_rv4Backward
echo "Backward"
cat diff_rv4Backward | grep "P "

#ForgottenOne
./a.out resources/tests/ForgottenOne.cor -r > rForgottenOne
./resources/vm_champs/corewar resources/tests/ForgottenOne.cor -v 4 > v4ForgottenOne
